node ("x86_runner1"){
    stage('Cleanup'){
		sh'''#!/bin/bash
			rm -rf $HOME/ci_test--jiewu
		'''
        cleanWs()
    }
    stage('60s Sleep'){
        sleep(time:60, unit:'SECONDS')
    }
    stage('Checkout x86'){
        checkout scm
    }
    stage ('run add'){
        sh'''#!/bin/bash
            python3 "getresult.py"
            res1=$?
            echo $res1
            if[$res1==0];then
              python3 "add.py"
              res2=$?
              echo $res2
            else
              echo "failed"
              exit 8 
            fi    
        '''
    }
    stage('Cleanup'){
		sh'''#!/bin/bash
			rm -rf $HOME/ci_test--jiewu
		'''
        cleanWs()
    }
}
